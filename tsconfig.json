{
    "compilerOptions": {
        "outDir": "dist",
        "module": "commonjs",
        "target": "ES2019",
        "lib": ["DOM", "DOM.Iterable", "WebWorker", "ES2019"],
        "alwaysStrict": true,
        "strict": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "esModuleInterop": true,
        "types": ["@cloudflare/workers-types"]
    },
    "include": [
        "./node_modules/@cloudflare/workers-types/index.d.ts",
        "./*.ts",
        "./**/*.ts"
    ],
    "exclude": ["node_modules/"]
}
